{
  "cli": {
    "version": ">= 16.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "apk"
      },
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "apk"
      },
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "app-bundle"
      },
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "yaskhalil06@icloud.com",
        "appleTeamId": "6CL27WHBRU"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    },
    "production_asc_api_key": {
      "ios": {
        "appleTeamId": "6CL27WHBRU",
        "ascApiKeyPath": "./asc-api-key.p8",
        "ascApiKeyId": "8M9PVK6L2F",
        "ascApiKeyIssuerId": "12bc3aa3-c678-403c-b477-15b53438152e"
